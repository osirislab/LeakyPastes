        Scanner scanner = new Scanner(System.in);

        int startingYield = Integer.parseInt(scanner.nextLine());

        int totalAmount = 0;
        int countDays = 0;

        if (startingYield < 100) {
            System.out.println(countDays);
            System.out.println(totalAmount);
        } else {
