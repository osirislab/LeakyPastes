;EBX = Address of GetProcAddress(...)
;ECX = Image Base of KERNEL32.DLL
	@DELTA	esi
	mov		ebx, eax					;Address of GetProcAddress(...)
	mov		ecx, dwKernelBase			;Base address of KERNEL32.DLL
